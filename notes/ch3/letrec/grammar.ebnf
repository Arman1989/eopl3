Program ::= Expr
Expr    ::= Const | Var | Diff | Zero | If | Let | Letrec | Proc | Call
Const   ::= Number
Var     ::= Id
Diff    ::= '-' '(' Expr ',' Expr ')'
Zero    ::= 'zero?' '(' Expr ')'
If      ::= 'if' Expr 'then' Expr 'else' Expr
Let     ::= 'let' Id '=' Expr 'in' Expr
Letrec  ::= 'letrec' Id '(' Id ')' '=' Expr 'in' Expr
Proc    ::= 'proc' '(' Id ')' Expr
Call    ::= '(' Expr Expr ')'
Number  ::= '-'? [0-9]+
Id      ::= [a-z]+
